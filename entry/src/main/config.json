            "supportDimensions": [
              "2*4",
              "4*4"
            ],
            "portraitLayouts": [
              "$layout:form_list_pattern_widget4_2_4",
              "$layout:form_list_pattern_widget4_4_4"
            ],
            "updateEnabled": true,
            "updateDuration": 1
          },
          {
            "landscapeLayouts": [
              "$layout:form_list_pattern_widget5_2_4",
              "$layout:form_list_pattern_widget5_4_4"
            ],
            "isDefault": false,
            "scheduledUpdateTime": "10:30",
            "defaultDimension": "4*4",
            "name": "widget5",
            "description": "This is a service widget5",
            "colorMode": "auto",
            "type": "Java",
            "supportDimensions": [
              "2*4",
              "4*4"
            ],
            "portraitLayouts": [
              "$layout:form_list_pattern_widget5_2_4",
              "$layout:form_list_pattern_widget5_4_4"
            ],
            "updateEnabled": true,
            "updateDuration": 1
          }
        ]
      },
      {
        "name": "com.example.mycard.SecondAbility",
        "icon": "$media:icon",
        "description": "$string:secondability_description",
        "label": "$string:entry_SecondAbility",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "default",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": true
        }
      },
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "widget1",
